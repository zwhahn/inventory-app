<html>
  <head>
    <title>Inventory App</title>
    <link rel="stylesheet" href="/styles.css">
  </head>
  <body>
    <h1>Cateogories</h1>
    <a href="/products"><button>All Products</button></a>
    <ul>
      <% categories.forEach(category => { %>
        <a href=<%=`/products/${category.id}`%>>
          <li>
            <%= category.category %>
          </li>
        </a>
      <% }); %>
    </ul>
  </body>
</html>